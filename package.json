    "start:watch": "nodemon src/index.ts"
  },
  "devDependencies": {
    "@types/fhir": "^0.0.35",
    "@types/node": "^18.11.5",
    "@typescript-eslint/eslint-plugin": "^5.45.0",
    "@typescript-eslint/parser": "^5.45.0",
    "eslint": "^8.28.0",
    "eslint-config-prettier": "^8.5.0",
    "nodemon": "^2.0.20",
    "prettier": "^2.8.0",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.3"
  },
  "dependencies": {
    "@projecttacoma/node-fhir-server-core": "^2.2.8"
  }
